1.
НАИМЕНОВАНИЕ ПРАВИЛА 'УК РФ, гл.28, ст.273, п.1'
ЕСЛИ
    'уничтожение информации' OR
    'блокирование информации' OR
    'модификация информации' OR
    'копирование информации'
ТО
    'штраф 200' XOR
    'штраф в размере заработной платы до 18' XOR
    'лишение свободы 24' XOR
    'исправительные работы 12' XOR
    'ограничение свободы 24' XOR
    'принудительные работы 24'

2.
НАИМЕНОВАНИЕ ПРАВИЛА 'УК РФ, гл.28 ст.273, п.2'
ЕСЛИ
    'крупный ущерб' AND
    (
        'уничтожение информации' OR
        'блокирование информации' OR
        'модификация информации' OR
        'копирование информации'
    )
ТО
    'штраф 100, 300' XOR
    'штраф в размере заработной платы 12, 24' XOR
    'исправительные работы 12, 24' XOR
    'лишение свободы 48' (10%) XOR
    'ограничение свободы 48' XOR
    'принудительные работы 48'

3.
НАИМЕНОВАНИЕ ПРАВИЛА 'УК РФ, гл.28, ст.273, п.3'
ЕСЛИ
    'совершено группой лиц' AND
    (
        'уничтожение информации' OR
        'блокирование информации' OR
        'модификация информации' OR
        'копирование информации'
    )
ТО
    'штраф до 500' XOR
    (
        'штраф в размере заработной платы до 36' AND
        'лишение права занимать определенные должности'
    ) XOR
    'лишение свободы 60' XOR
    'принудительные работы 60' XOR
    'ограничение свободы 48'
4.
НАИМЕНОВАНИЕ ПРАВИЛА 'УК РФ, гл.1, ст.274, п.1'
ЕСЛИ
    'создание вредоносных компьютерных программ' OR
    'использование вредоносных компьютерных программ' OR
    'распространение вредоносных компьютерных программ'
ТО
    'ограничение свободы 48' XOR
    'принудительные работы 48' XOR
    (
        'лишение свободы 48' AND
        (
            'штраф 200' XOR
            'штраф в размере зарабатной платы до 18'
        )
    )
5.
НАИМЕНОВАНИЕ ПРАВИЛА 'УК РФ, гл.1, ст.274, п.2'
ЕСЛИ
    (
        (
            'совершенные группой лиц' AND
            'совершено по предварительному сговору'
        ) XOR
        'совершено организованной группой' XOR
        'совершено лицом с использованием своего служебного положения'
    ) AND
    (
        'создание вредоносных компьютерных программ' OR
        'использование вредоносных компьютерных программ' OR
        'распространение вредоносных компьютерных программ'
    )
ТО
    'ограничение свободы 48' XOR
    (
        'принудительные работы 60' AND
         (
            'лишение права занимать определенные должности' XOR
            'лишение права заниматься определенной деятельностью до 36'
         )
    ) XOR
    (
        'лишением свободы 60' AND
        (
            'штраф 100, 200' XOR
            'штраф в размере заработной платы 24, 36'
        ) AND
        (
            'лишение права занимать определенные должности' XOR
            'лишение права заниматься определенной деятельностью до 36'
        )
    )
